body{color:#666666;"����";}
a,img{border:0;text-decoration:none;}
em{font-style:normal;}
ol, ul{list-style:none outside none;padding:0;margin:0;}

.tab li, .tab a, .tab-item{float:left;text-align:center;}
.tab li, .tab a, .tab-item{cursor:pointer;float:left;text-align:center;}
#summary-stock3 .dt{margin-top:4px;}
#list3 .dt{float:left;height:18px;text-align:right;width:72px;}
/*#list1 .dd{width:324px;float:left;}*/

/*#store-selector3{position:relative;float:left;z-index:2;height:26px;margin-right:6px}*/
#store-selector3{position:relative;z-index:2;}
#store-selector3 dl,#store-selector3 dt,#store-selector3 dd{float:none;color:#999}
#store-selector3 dl{position:absolute;top:23px;z-index:1;width:358px;width:358px;padding:5px;border:1px solid #ddd;background:#fff;display:none;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#store-selector3 dt{padding:6px 0 10px;color:#999}
#store-selector3 dd{padding-bottom:5px;line-height:18px}
#store-selector3 a:link,#store-selector3 a:visited{color:#005aa0}
#store-selector3 a:hover,#store-selector3 a:active{background:#005aa0;color:#fff}
/*
#store-selector3 .text{float:left;+float:none;_float:left;position:relative;top:0;z-index:2;height:23px;background:#fff;border:1px solid #CECBCE;padding:0 20px 0 4px;line-height:23px;overflow:hidden}
*/
#store-selector3 .text b{display:block;position:absolute;top:0;right:0;overflow:hidden;width:17px;height:24px;background:url(../../images/btn20121210.png) 0 0 no-repeat}
#store-selector3 .close{display:none;position:absolute;z-index:2;top:33px;left:430px;cursor:pointer;width:17px;height:17px;background:url(../../images/20120418.png) no-repeat -40px 0}
#store-selector3 a:hover{background:#B79567;color:#fff;cursor:pointer}
#store-selector3.hover .text{border-bottom:0}
#store-selector3.hover .i-storeinfo{position:relative;z-index:2;background:url(img/bg_store.gif) no-repeat right -50px}
#store-selector3.hover .i-storeinfo div{background:url(img/bg_store.gif) no-repeat -81px -75px}
#store-selector3.hover dl{display:block}
#store-selector3.hover .close{display:block}

#store-prompt3{clear:left;line-height:25px}
#store-prompt3 strong{font-size:14px}
#store-prompt3 a{color:#005EAA}
*html #store-selector3 .i-storeinfo,*html #store-selector3 .i-storeinfo div{float:left}
*html #store-selector3 dl{left:0}
#store-selector3 .content{display:none;position:absolute;border:1px solid #cecbce;width:450px;padding:15px;background:#fff;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#store-selector3 .content select{float:left;width:120px;border:1px solid #cecbce;margin-right:15px}
#store-selector3 .content .select3{margin-right:0}
#store-selector3.hover .content,#store-selector3.hover .close{display:block}
#JD-stock3{position:relative;margin-bottom:0}
#JD-stock3 .tab{width:100%;height:25px;float:left;border-bottom:2px solid #edd28b;overflow:visible;*overflow:hidden}
#JD-stock3 .tab li{float:left;clear:none;height:23px;padding:1px 1px 0;border:1px solid #ddd;border-bottom:0;margin-right:3px;background-color:#fff;line-height:22px;text-decoration:none}
#JD-stock3 .tab .curr{*position:relative;height:25px;padding:0;border:2px solid #edd28b;border-bottom:0}
#JD-stock3 .tab a{position:relative;float:left;height:23px;padding:0 20px 1px 10px;line-height:23px;text-align:center;text-decoration:none;cursor:pointer;color:#005AA0;outline:0;*blr:expression(this.onFocus=this.blur())}
#JD-stock3 .tab a:hover{background:0;color:#005AA0}
#JD-stock3 .tab a i{position:absolute;right:5px;top:10px;*top:9px;display:block;width:7px;height:5px;overflow:hidden;background:url(../../images/20130606B.png) no-repeat -76px -34px;opacity:.5;filter:alpha(opacity=50)}
#JD-stock3 .tab .curr i,#JD-stock3 .tab a:hover i{opacity:1;filter:alpha(opacity=100)}
#JD-stock3 .area-list{padding-top:5px}
#JD-stock3 .area-list li{float:left;width:100px;padding:2px 0 2px 15px;clear:none}
#JD-stock3 .area-list li a{float:left;padding:2px 4px;*padding:0 4px;color:#005aa0}
#JD-stock3 .area-list li a:hover{color:#fff}
#JD-stock3 .area-list .longer-area{width:370px}
#JD-stock3 .area-list .long-area{width:170px}

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
    clear: both;
}
.clearfix {
    *zoom: 1;
}
