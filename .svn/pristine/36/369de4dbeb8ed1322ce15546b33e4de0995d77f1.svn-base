        "currentTownName": ""
    };
    var ipLoc = getCookie("ipLoc-djd");
    ipLoc = ipLoc ? ipLoc.split("-") : [100100, 1001001000, 0, 0];
    if (ipLoc.length > 0 && ipLoc[0]) {
