    if (ipLoc.length > 1 && ipLoc[1]) {
        currentAreaInfo.currentCityId = ipLoc[1];
    }
    if (ipLoc.length > 2 && ipLoc[2]) {
        currentAreaInfo.currentAreaId = ipLoc[2];
    }
    if (ipLoc.length > 3 && ipLoc[3]) {
        currentAreaInfo.currentTownId = ipLoc[3];
    }
}
